            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(3017.20044, 19.2317762, 929.637634, -0.444271803, -8.99723746e-05, -0.895892024, -2.76411065e-05, 1, -8.67205017e-05, 0.895892024, -1.37640227e-05, -0.444271803)
