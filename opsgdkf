local Misc = Window:MakeTab({
	Name = "Misc",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Misc:AddSlider({
	Name = "Walk Speed",
	Min = 16,
	Max = 250,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Speed",
    Callback = function(v)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
    end;
})

Misc:AddSlider({
	Name = "Jump Power",
	Min = 50,
	Max = 350,
	Default = 50,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Jump",
    Callback = function(v)
        game.Players.LocalPlayer.Character.Humanoid.JumpHeight = v
    end;
})

Misc:AddSlider({
	Name = "FOV",
	Min = 70,
	Max = 120,
	Default = 70,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Distance",
    Callback = function(v)
        game.Workspace.CurrentCamera.FieldOfView = v
    end;
})
end

Key:AddParagraph("Discord","https://discord.gg/NcPVhg9fbz")

Key:AddTextbox({
	Name = "Enter Key",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		getgenv().KeyInput = Value
	end	  
})

Key:AddButton({
	Name = "Check",
	Callback = function()
      	if getgenv().KeyInput == getgenv().Key then
        Printer()
        CorrectKeyNotificacion()
        else
            IncorrectKeyNotificacion()
        end
  	end    
})

function IncorrectKeyNotificacion()
    OrionLib:MakeNotification({
        Name = "Check your key again",
        Content = "You entered an incorrect key",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end

function CorrectKeyNotificacion()
    OrionLib:MakeNotification({
        Name = "Congratulations!",
        Content = "You entered the correct key",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end
