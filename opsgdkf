local time = 0.01 

click = false
m = game.Players.LocalPlayer:GetMouse()
m.KeyDown:connect(function(key)
if key == "v" then
if click == true then click = false
elseif
click == false then click = true

while click == true do 
wait(time)
mouse1click()
end
end
end
end)

Tab:AddToggle({
	Name = "AutoBoss",
	Default = false,
	Callback = function(v)
        getgenv().autoBoss = v
        autoBoss()
	end    
})

Tab:AddParagraph("AutoSlice","To turn on/off the AutoSlice with the key : v")

local Teleport = Window:MakeTab({
	Name = "Teleport",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Teleport:AddDropdown({
	Name = "Upgrade Zones",
	Default = "",
	Options = {"1", "2", "3"},
	Callback = function(Value)
		if Value == "1" then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(8.6030407, 9.37103939, 637.759338, -0.623441279, -0.00841447711, 0.781824887, 0.0197153166, 0.999454916, 0.0264781713, -0.781621516, 0.031921491, -0.622935593)
