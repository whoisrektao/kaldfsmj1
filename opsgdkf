            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-3628.41504, 39.3653145, 519.767029, 0.641203284, -3.03136821e-10, 0.767371058, 4.8592852e-10, 1, -1.1001402e-11, -0.767371058, 3.79941634e-10, 0.641203284)
        end
    end
})

Teleport:AddDropdown({
	Name = "Useful Teleports",
	Default = "",
	Options = {"Spawn", "SuperRebirth", "Portal Hub"},
	Callback = function(Value)
		if Value == "Spawn" then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(58.3560524, 7.0998683, 357.861511, -0.848873794, -2.07865654e-08, 0.528595567, 7.31962002e-09, 1, 5.10787466e-08, -0.528595567, 4.72285286e-08, -0.848873794)
