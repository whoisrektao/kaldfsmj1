Key:AddParagraph("Discord","https://discord.gg/NcPVhg9fbz")

Key:AddTextbox({
	Name = "Enter Key",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		getgenv().KeyInput = Value
	end	  
})

Key:AddButton({
	Name = "Check",
	Callback = function()
      	if getgenv().KeyInput == getgenv().Key then 
        Printer()
        discordlogfree()
        CorrectKeyNotificacion()
        elseif getgenv().KeyInput == getgenv().PermanentKeyStaff then
        Printer()
        discordlog()
        CorrectKeyNotificacion()
        else
            IncorrectKeyNotificacion()
        end
  	end    
})

function IncorrectKeyNotificacion()
    OrionLib:MakeNotification({
        Name = "Check your key again",
        Content = "You entered an incorrect key",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end

function CorrectKeyNotificacion()
    OrionLib:MakeNotification({
        Name = "Congratulations!",
        Content = "You entered the correct key",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end

getgenv().PermanentLog = "Private Key has been used"

function discordlog()
local webh = "https://discord.com/api/webhooks/1041456446157635635/F8K0d2_5E5fnlDCIdhdOp0sd0dQbAR0z-LtnKJAIq5SDMOuHv8mBN5AQtNH-57HnU2Iu"

   local data = {

  ['embeds'] = {
    {
       ['type'] = "rich",
       ['fields'] = {
          {name = '**Username**', value = game:GetService("Players").LocalPlayer.Name},
          {name = "**Key**", value = (getgenv().PermanentLog)},
        }
    }
  }
}

   if syn then
       local response = syn.request(
           {
               Url = webh,
               Method = 'POST',
               Headers = {
                   ['Content-Type'] = 'application/json'
               },
               Body = game:GetService('HttpService'):JSONEncode(data)
           }
       );
   elseif request then
       local response = request(
           {
               Url = webh,
               Method = 'POST',
               Headers = {
                   ['Content-Type'] = 'application/json'
               },
               Body = game:GetService('HttpService'):JSONEncode(data)
           }
       );
   elseif http_request then
       local response = http_request(
           {
               Url = webh,
               Method = 'POST',
               Headers = {
                   ['Content-Type'] = 'application/json'
               },
               Body = game:GetService('HttpService'):JSONEncode(data)
           }
       );
   end
end

getgenv().FreeLog = "Free Key has been used"

function discordlogfree()
    local webh = "https://discord.com/api/webhooks/1041456446157635635/F8K0d2_5E5fnlDCIdhdOp0sd0dQbAR0z-LtnKJAIq5SDMOuHv8mBN5AQtNH-57HnU2Iu"

    local data = {
 
   ['embeds'] = {
     {
        ['type'] = "rich",
        ['fields'] = {
           {name = '**Username**', value = game:GetService("Players").LocalPlayer.Name},
           {name = "**Key**", value = (getgenv().FreeLog)},
         }
     }
   }
 }
 
    if syn then
        local response = syn.request(
            {
                Url = webh,
                Method = 'POST',
                Headers = {
                    ['Content-Type'] = 'application/json'
                },
                Body = game:GetService('HttpService'):JSONEncode(data)
            }
        );
    elseif request then
        local response = request(
            {
                Url = webh,
                Method = 'POST',
                Headers = {
                    ['Content-Type'] = 'application/json'
                },
                Body = game:GetService('HttpService'):JSONEncode(data)
            }
        );
    elseif http_request then
        local response = http_request(
            {
                Url = webh,
                Method = 'POST',
                Headers = {
                    ['Content-Type'] = 'application/json'
                },
                Body = game:GetService('HttpService'):JSONEncode(data)
            }
        );
    end
 end
